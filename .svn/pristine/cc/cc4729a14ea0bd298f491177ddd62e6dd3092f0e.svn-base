/*reset*/
body,
p,
div,
h1,
h2,
h3,
h4,
h5,
h6,
li,
ol,
ul,
dl,
dt,
dd,
span,
th,
td,
form,
input,
button,
pre {
  margin: 0;
  padding: 0;
  font-family: Arial, 'Microsoft YaHei';
}
html {
  height: 100%;
}
body {
  height: 100%;
  background: #303031;
  line-height: 1.7;
  font-size: 14px;
  font-family: Arial, "Microsoft YaHei";
  color: #333;
}
a {
  text-decoration: none;
  color: #8fcdfb;
}
ul li,
ol li,
li {
  list-style: none;
}
a img {
  border: none;
}
table {
  border-collapse: collapse;
}
h1,
h2,
h3,
h4,
h5,
h6,
table th {
  font-size: 100%;
  font-weight: normal;
}
i,
b,
em,
small {
  font-weight: normal;
  font-style: normal;
}
.default {
  color: #999;
}
.po-re {
  position: relative;
}
.clearfix {
  overflow: hidden;
  _zoom: 1;
}
.pull-left {
  float: left;
}
.pull-right {
