/* alert弹出层*/
#alert {
  background-color: rgba(0, 0, 0, 0.5);
  display: none;
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  z-index: 10000;
}
#alert .Dialog {
  padding: 15px 20px 3px;
  cursor: pointer;
  border-radius: 2px;
  background: white;
  position: fixed;
  top: 50%;
  left: 50%;
  width: 440px;
  height: 227px;
  margin-left: -160px;
  margin-top: -60px;
  color: #eef2f5;
}
#alert .closeMe {
  position: absolute;
  right: 10px;
  top: 7px;
  cursor: pointer;
  border-bottom: 1px solid #eef2f5;
  font-size: 34px;
  font-wegiht: bold;
}
#alert .msg-content {
  text-align: center;
  padding: 50px 0px;
  color: #666;
  font-size: 18px;
}
#alert .handler-box {
  padding: 10px;
  text-align: center;
}
#alert .handler-box .ok-btn {
  margin-right: 10px;
  background-color: #48b1fb;
  border: 1px solid #48b1fb;
  color: #fff;
  width: 92px;
  height: 32px;
  border-radius: 2px;
  cursor: pointer;
}
#alert .handler-box .cancel-btn {
  display: none;
  background-color: #aeb5bf;
  border: 1px solid #aeb5bf;
  color: #fff;
  width: 80px;
  height: 25px;
  border-radius: 2px;
  cursor: pointer;
}
#alert .FCcolor {
  color: #666;
  font-size: 14px;
  height: 27px;
  line-height: 27px;
  text-align: center;
  border-bottom: 1px solid #eef2f5;
  padding-bottom: 10px;
}
.marketing {
  cursor: pointer;
  color: #48b1fb;
}
