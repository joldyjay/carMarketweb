/*get tip box*/
.scrollBox ul.con .arrow{
  position:absolute;
  width:5px;
  height:10px;
  top: -12px;
  left: 22px;
  /*left: 182px;*/
  -moz-transition: all 0.5s ease-in;
  -webkit-transition: all 0.5s ease-in;
  -o-transition: all 0.5s ease-in;
  transition: all 0.5s ease-in;
}
.scrollBehavior .arrow1{
  position:absolute;
  width:5px;
  height:10px;
  top: -12px;
  left: 93px;
  /*left: 162px;*/
  -moz-transition: all 0.5s ease-in;
  -webkit-transition: all 0.5s ease-in;
  -o-transition: all 0.5s ease-in;
  transition: all 0.5s ease-in;
}
.labelScrollBox .arrow2{
  position:absolute;
  width:5px;
  height:10px;
  top: -12px;
  left: 94px;
  /*left: 162px;*/
  -moz-transition: all 0.5s ease-in;
  -webkit-transition: all 0.5s ease-in;
  -o-transition: all 0.5s ease-in;
  transition: all 0.5s ease-in;
}
.scrollBox ul.con .arrow *{
  display:block;
  border-width:6px;
  position:absolute;
  border-style:solid dashed dashed dashed;
  font-size:0;
  line-height:0;
}
.scrollBehavior .arrow1 * {
  display:block;
  border-width:6px;
  position:absolute;
  border-style:solid dashed dashed dashed;
  font-size:0;
  line-height:0;
}
.labelScrollBox .arrow2 * {
  display:block;
  border-width:6px;
  position:absolute;
  border-style:solid dashed dashed dashed;
  font-size:0;
  line-height:0;
}
.scrollBox ul.con .arrow em,
.scrollBehavior .arrow1 em,
.labelScrollBox .arrow2 em{
  border-color: transparent transparent #ccc;
}

.scrollBox ul.con .arrow span,
.scrollBehavior .arrow1 span,
.labelScrollBox .arrow2 span{
  border-color: transparent transparent #FFF;
  top:1px;
}

