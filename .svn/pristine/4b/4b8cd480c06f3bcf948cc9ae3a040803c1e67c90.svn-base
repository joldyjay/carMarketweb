  background: transparent;
}
.caseCon.disable {
  border: 0;
  color: #666;
  font-size: 14px;
}
.caseCon.disable:disabled {
  background: transparent;
}
#box {
  height: 150px;
  overflow: hidden;
  position: relative;
}
