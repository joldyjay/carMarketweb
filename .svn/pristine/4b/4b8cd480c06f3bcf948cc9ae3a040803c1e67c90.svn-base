/*radio*/
.my-radio [type="radio"],
.my-check [type="checkbox"]{
  display: none;
}
.my-radio i{
  background: url(../../assets/imgs/click.png) no-repeat center center;
  display:inline-block;
  width:14px;
  height:14px;
  vertical-align:middle;
  margin-left: 5px;
}
.my-check i{
  background: url(../../assets/imgs/checkbox.png) 0 0 no-repeat;
  display:inline-block;
  width:16px;
  height:15px;
  vertical-align:middle;
  margin-left: 5px;
}
.my-radio span,
.my-check span{
  vertical-align: middle;
  padding-left:5px;
  font-size: 12px;
}
.my-radio [type="radio"]:checked+i{
  background: url(../../assets/imgs/click-u.png) no-repeat center center;
}

.my-check [type="checkbox"]:checked+i{
  background: url(../../assets/imgs/checkbox.png) 0 -15px no-repeat;
}

