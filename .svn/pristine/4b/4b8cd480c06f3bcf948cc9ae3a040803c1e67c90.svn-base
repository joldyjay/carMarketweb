.table {
  border: 1px solid #eef2f5;
  width: 100%;
  font-size: 12px;
  text-align: center;
  table-layout: fixed;
}
.table thead tr {
  height: 45px;
  background: #f1f3f3;
  color: #777;
}
.table tbody tr {
  height: 40px;
  color: #333;
  border-top: 1px solid #eef2f5;
}
.table tbody td {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding: 0 5px;
}
input,
select {
  height: 30px;
  border: 1px solid #ccc;
  line-height: 30px;
  padding: 0 10px;
  font-size: 12px;
  border-radius: 2px;
  color: #333;
  font-family: 'Microsoft YaHei', Arial;
}
input:disabled,
select:disabled {
  background: #eef2f5;
}
textarea {
  border: 1px solid #ccc;
  padding: 5px 10px;
  font-size: 12px;
  border-radius: 2px;
  color: #333;
  font-family: 'Microsoft YaHei', Arial;
  resize: none;
}
label {
  vertical-align: top;
  display: inline-block;
  font-size: 14px;
  color: #666;
  margin-top: 5px;
  width: 99px;
  text-align: right;
  font-weight:normal!important;
}
