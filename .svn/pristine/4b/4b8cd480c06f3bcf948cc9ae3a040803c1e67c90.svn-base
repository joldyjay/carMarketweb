  height: 300%;
  margin: 0px auto;
}
.msgPushArea h4 {
  padding: 0 0 20px 0;
}
.msgPushArea .r {
  width: 60%;
}
.msgPushArea .scroll {
  position: relative;
  width: 320px;
  overflow: hidden;
  justify-content: center;
}
.msgPushArea .l {
  margin-right: 20px;
  border-right: 1px #ccc solid;
}
.msgPushArea .scroll .scroll_left {
  width:13px;
  height:45px;
  position: absolute;
  left: 0;
  top: 100px;
  z-index: 10;
  background: url('../imgs/btn_left.png') center no-repeat;
  display:block;
  -moz-transition: all 0.5s ease-in;
  -webkit-transition: all 0.5s ease-in;
  -o-transition: all 0.5s ease-in;
  transition: all 0.5s ease-in;
}
.msgPushArea .scroll .scroll_left.noImg{
  background: url('../imgs/btn_left_no.png') center no-repeat;
  -moz-transition: all 0.5s ease-in;
  -webkit-transition: all 0.5s ease-in;
  -o-transition: all 0.5s ease-in;
  transition: all 0.5s ease-in;
}
.msgPushArea .scroll .scroll_right {
  position: absolute;
  right: 20px;
  top: 100px;
  width:13px;
  height:45px;
  z-index: 10;
  background: url('../imgs/btn_right.png') no-repeat;
  -moz-transition: all 0.5s ease-in;
  -webkit-transition: all 0.5s ease-in;
  -o-transition: all 0.5s ease-in;
  transition: all 0.5s ease-in;
}
.msgPushArea .scroll .scroll_right.noImg{
  background: url('../imgs/btn_right_no.png') no-repeat;
  -moz-transition: all 0.5s ease-in;
  -webkit-transition: all 0.5s ease-in;
  -o-transition: all 0.5s ease-in;
  transition: all 0.5s ease-in;
}
