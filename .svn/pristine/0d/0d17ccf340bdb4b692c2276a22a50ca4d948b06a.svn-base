    Object.getPrototypeOf = function getPrototypeOf(object) {
        return object.__proto__;
    };
</script>
<![endif]-->
<script src="../../assets/lib/require.js"></script>
<script src="../../assets/dist/config.js"></script>
<script>require(['../../assets/js/msg_push'])</script>
</body>
</html>
