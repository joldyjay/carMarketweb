/*----------*/
.none {
    display: none;
}
.dateWrap {
    position:relative;
    width: 100%;
    height: 100%;
    text-align: center;
}
.month1 {
    border-collapse:collapse;
}
.dateWrap .date-picker-wrapper.single-date {
    width: 500px;
}
.dateWrap .month-wrapper {
    width: 100%!important;
}
.dateWrap .week-name {
    background: #efefef;
    border-radius: 3px;
}
.dateWrap .date-picker-wrapper .month-wrapper table {
    width: 100%;
    float: left;
}
.dateWrap .date-picker-wrapper .month-wrapper table tr td {
    padding: 10px 15px;
    border: 1px #ccc solid;
}

.dateWrap .lunarDate {
    position: relative;
    width: 30px;
    padding: 5px 0 0 0;
}
.dateWrap .lunarDate div {
    padding: 5px 0 0 0;
}
.dateWrap .pushEnd .lunarDate .cornerTip {
    position: absolute;
    display: inline-block;
    height:0px;
    width:0px;
    top: -15px;
    right: -15px;
    border-top:solid 38px #ccc;
    border-left:solid 38px transparent;
    z-index: 10;
}
.dateWrap .pushEnd .lunarDate .cornerTip:after{
    /*content:"\2714";*/
    content:"\2715";
    font-size:14px;
    position: absolute;
    top: -32px;
    left: -16px;
    color: #fff;
}
.dateWrap .pushing .lunarDate .cornerTip {
    position: absolute;
    display: inline-block;
    height:0px;
    width:0px;
    top: -15px;
    right: -15px;
    border-top:solid 38px #26b273;
    border-left:solid 38px transparent;
    z-index: 10;
}
.dateWrap .pushing .lunarDate .cornerTip:after{
    /*content:"\2714";*/
    content:"\2714";
    font-size:14px;
    position: absolute;
    top: -32px;
    left: -16px;
    color: #fff;
}
.dateWrap .pushReady .lunarDate .cornerTip {
    position: absolute;
    display: inline-block;
    height:0px;
    width:0px;
    top: -15px;
    right: -15px;
    border-top:solid 38px #15B2E9;
    border-left:solid 38px transparent;
    z-index: 10;
}
.dateWrap .pushReady .lunarDate .cornerTip:after{
    /*content:"\2714";*/
    content:"\266C";
    font-size:14px;
    position: absolute;
    top: -32px;
    left: -16px;
    color: #fff;
}

.month-wrapper .pushArea {
    margin: 0px auto;
    text-align: center;
    width: 100%;
    clear: both;
    padding: 10px 0 0 0;
}
.month-wrapper .pushArea ul li {
    float: left;
    width: 33.333%;
    font-color: #222!important;
    display: inline-block;
    text-align: center;
}

.month-wrapper .pushArea ul li i.endIcon:after{
    /*content:"\2714";*/
    content:"\2715";
    font-size:14px;
    top: 0;
    left: 0;
    color: red;
    width: 20px;
    height: 20px;
    font-weight: bold;
    display: inline-block;
}
.month-wrapper .pushArea ul li i.pushingIcon:after{
    content:"\2714";
    font-size:14px;
    top: 0;
    left: 0;
    color: #26b273;
    width: 20px;
    height: 20px;
    font-weight: bold;
    display: inline-block;
}
.month-wrapper .pushArea ul li i.readyIcon:after{
    /*content:"\2714";*/
    content:"\266C";
    font-size:14px;
    top: 0;
    left: 0;
    color: red;
    width: 20px;
    height: 20px;
    font-weight: bold;
    display: inline-block;
}