/*----------*/
.none {
    display: none;
}
.viewPeriod .modal-content {
    width: 700px;
}
.viewPeriod .modal-body {
    height: 500px;
    padding: 0;
}
.dateWrap {
    position:relative;
    height: 100%;
    text-align: center;
}
.viewPeriod .leftBar {
    position: relative;
    width: 170px;
    background: #EDF1F5;
    padding: 40px 20px;
}
.viewPeriod .leftBar,
.viewPeriod .dateWrap {
    float: left;
}
/* point up */
.leftBar .pointUp, .leftBar .pointDown {
    position: relative;
    width: 100%;
    height: 50px;
    text-align: center;
    cursor: pointer;
    /* left: 182px; */
    -moz-transition: all 0.5s ease-in;
    -webkit-transition: all 0.5s ease-in;
    -o-transition: all 0.5s ease-in;
    transition: all 0.5s ease-in;
}
.leftBar .pointUp i.no {
    display: inline-block;
    width: 14px;
    height: 8px;
    background: url('../imgs/touchPointIcon.png') 0 -10px no-repeat;
}
.leftBar .pointUp i {
    display: inline-block;
    width: 14px;
    height: 8px;
    background: url('../imgs/touchPointIcon.png') 0 0 no-repeat;
    -moz-transition: all 0.5s ease-in;
    -webkit-transition: all 0.5s ease-in;
    -o-transition: all 0.5s ease-in;
    transition: all 0.5s ease-in;
}
.leftBar .pointDown {
    margin: 20px 0 0 0;
}
.leftBar .pointDown i.no {
    display: inline-block;
    width: 14px;
    height: 8px;
    background: url('../imgs/touchPointIcon.png') -18px -11px no-repeat;
}
.leftBar .pointDown i {
    display: inline-block;
    width: 14px;
    height: 8px;
    background: url('../imgs/touchPointIcon.png') -18px 0 no-repeat;
    -moz-transition: all 0.5s ease-in;
    -webkit-transition: all 0.5s ease-in;
    -o-transition: all 0.5s ease-in;
    transition: all 0.5s ease-in;
}
/* up arrow */
.leftBar .up {
    position: relative;
    width: 100%;
    height: 50px;
    text-align: center;
    margin: 0 0 20px 0;
    /* left: 182px; */
    -moz-transition: all 0.5s ease-in;
    -webkit-transition: all 0.5s ease-in;
    -o-transition: all 0.5s ease-in;
    transition: all 0.5s ease-in;
}
.leftBar .up em
{
    border-color: transparent transparent #fff;
    left: 43px;
}
.leftBar .up * {
    display: block;
    border-width: 10px;
    position: absolute;
    border-style: solid dashed dashed dashed;
    font-size: 0;
    line-height: 0;
    top: 0;
}
.leftBar .up span
{
    border-color: transparent transparent #EDF1F5;
    top: 2px;
    left: 43px;
}
/* down arrow */
.leftBar .down {
    position: relative;
    width: 100%;
    height: 50px;
    text-align: center;
    margin: 0 0 20px 0;
    /* left: 182px; */
    -moz-transition: all 0.5s ease-in;
    -webkit-transition: all 0.5s ease-in;
    -o-transition: all 0.5s ease-in;
    transition: all 0.5s ease-in;
}
.leftBar .down em
{
    border-color: #fff transparent transparent;
    left: 43px;
}
.leftBar .down * {
    display: block;
    border-width: 10px;
    position: absolute;
    border-style: solid dashed dashed  dashed;
    font-size: 0;
    line-height: 0;
    bottom: 0;
}
.leftBar .down span
{
    border-color: #EDF1F5 transparent transparent;
    bottom: 2px;
    left: 43px;
}

.month1 {
    border-collapse:collapse;
}

.viewPeriod .leftBar .lbWrap {
    float: left;
    height: 330px;
    font-size: 14px;
    overflow: hidden;
    text-align: center;
}
.viewPeriod .leftBar .lbWrap .lbInner {
    float: left;
    width: 100%;
    margin: 0px auto;
}
.viewPeriod .leftBar .lbWrap .lbInner ul > li {
    float: left;
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
}
.viewPeriod .leftBar .lbWrap .sel span {
    font-weight: bold;
}
.viewPeriod .leftBar .lbWrap .secondLevel li {
    text-align: center;
}
.dateWrap .date-picker-wrapper.single-date {
    width: 500px;
}
.dateWrap .month-wrapper {
    width: 100%!important;
}

.dateWrap .week-name {
    background: #efefef;
    border-radius: 3px;
}
.dateWrap .date-picker-wrapper .month-wrapper table {
    width: 100%;
    float: left;
}
.dateWrap .date-picker-wrapper .month-wrapper table tr td {
    padding: 10px 15px;
    border: 1px #ccc solid;
}

.dateWrap .lunarDate {
    position: relative;
    width: 30px;
    padding: 5px 0 0 0;
}
.dateWrap .lunarDate div {
    padding: 5px 0 0 0;
}
.dateWrap .pushEnd .lunarDate .cornerTip {
    position: absolute;
    display: inline-block;
    height:0px;
    width:0px;
    top: -15px;
    right: -15px;
    border-top:solid 38px #ccc;
    border-left:solid 38px transparent;
    z-index: 10;
}
.dateWrap .pushEnd .lunarDate .cornerTip:after{
    /*content:"\2714";*/
    content:"\2715";
    font-size:14px;
    position: absolute;
    top: -32px;
    left: -16px;
    color: #fff;
}
.dateWrap .pushing .lunarDate .cornerTip {
    position: absolute;
    display: inline-block;
    height:0px;
    width:0px;
    top: -15px;
    right: -15px;
    border-top:solid 38px #26b273;
    border-left:solid 38px transparent;
    z-index: 10;
}
.dateWrap .pushing .lunarDate .cornerTip:after{
    /*content:"\2714";*/
    content:"\2714";
    font-size:14px;
    position: absolute;
    top: -32px;
    left: -16px;
    color: #fff;
}
.dateWrap .pushReady .lunarDate .cornerTip {
    position: absolute;
    display: inline-block;
    height:0px;
    width:0px;
    top: -15px;
    right: -15px;
    border-top:solid 38px #15B2E9;
    border-left:solid 38px transparent;
    z-index: 10;
}
.dateWrap .pushReady .lunarDate .cornerTip:after{
    float: left;
    content:"";
    width: 19px;
    height: 15px;
    background: url('../imgs/readyPushIcon.png')  center no-repeat;
    position: absolute;
    top: -33px;
    left: -16px;
}

.month-wrapper .pushArea {
    margin: 0px auto;
    text-align: center;
    width: 100%;
    clear: both;
    padding: 15px 0;
}
.month-wrapper .pushArea ul li {
    float: left;
    width: 33.333%;
    font-color: #222!important;
    display: inline-block;
    text-align: center;
}

.month-wrapper .pushArea ul li i.endIcon:after{
    /*content:"\2714";*/
    content:"\2715";
    font-size:14px;
    top: 0;
    left: 0;
    color: red;
    width: 20px;
    height: 20px;
    font-weight: bold;
    display: inline-block;
}
.month-wrapper .pushArea ul li i.pushingIcon:after{
    content:"\2714";
    font-size:14px;
    top: 0;
    left: 0;
    color: #26b273;
    width: 20px;
    height: 20px;
    font-weight: bold;
    display: inline-block;
}
.month-wrapper .pushArea ul li i.readyIcon {
    width: 61px;
    display: inline-block;
}
.month-wrapper .pushArea ul li i.readyIcon span.readtIcon_icon{
    content: ' ';
    float: left;
    width: 20px;
    height: 19px;
    background: url('../imgs/readyPushIcon_ins.png')  center no-repeat;
    display: inline-block;
}